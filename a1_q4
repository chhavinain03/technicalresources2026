#include<iostream>
using namespace std;

int main(){
    int n;
    cout<<"ENTER A NUMBER:";
    cin>>n;
    int i,j;
    int count=0;
    for(j=2;j<=n;j++){
        count=0;
    for(i=1;i<=j;i++){
        if(j%i==0){
            count++;
        }}
    if(count==2)
        cout<<j<<endl;
   /ese{
        cout<<"NUMBER IS NOT PRIME";
    }
    
}
}
